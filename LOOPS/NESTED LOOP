<!-- NESTED LOOP -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Loops</title>
</head>
<script>
    function main(){
       for(let i=1;i<=5;i++){
        for(let j=1;j<=5;j++){
            if(j==3 || i==3) return;
            console.log(`i=${i} , j=${j}`);
        }
       }

    }
    main();
</script>
<body>
    <h1>Loops statements</h1>
    
</body>
</html>
