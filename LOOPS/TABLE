<script>
    function main(){
        n=prompt("Enter number : ");
        for(let i=1;i<=10;i++){
            console.log(`${n} * ${i} = ${n*i}`);
        }

    }
    main();
</script>
<body>
    <h1>Loops statements</h1>
    
