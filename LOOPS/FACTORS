<!-- FACTORS OF A GIVEN NUMBER -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Loops</title>
</head>
<script>
    function main(){
        n=prompt("Enter number : ");
        for(let i=0;i<=n;i++){
            if(n%i==0) console.log(`${i} is the factor of ${n}`);
        }

    }
    main();
</script>
<body>
    <h1>Loops statements</h1>
    
</body>
</html>
