<!-- PRIME NUMBER FROM 1-10 -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Loops</title>
</head>
<script>
    function main(){
        for(let i=1;i<=10;i++){
            if(i%2==0) console.log(`${i} is an Even Number !`);
            else console.log(`${i} Not a Even Number`);
        }

    }
    main();
</script>
<body>
    <h1>Loops statements</h1>
    
</body>
</html>
